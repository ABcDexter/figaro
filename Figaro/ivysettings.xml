<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
  <!-- Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it. -->
  <credentials host="localhost" realm="Artifactory Realm" username="admin" passwd="password"/>
  <resolvers>
    <url name="cra-repo" m2compatible="true">
      <artifact pattern="http://repo.cra.com:8081/ivy/[organization]/[module]/[revision]/[artifact]-[revision].[ext]"/>
      <ivy pattern="http://repo.cra.com:8081/ivy/[organization]/[module]/[revision]/ivy-[revision].xml" />
    </url>
	<ibiblio name="main" m2compatible="true" root="http://repo.cra.com:8081/repo/"/>	
  </resolvers>
  <settings defaultResolver="default"/>
  <include url="${ivy.default.settings.dir}/ivysettings-local.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-default-chain.xml"/>
</ivysettings>
